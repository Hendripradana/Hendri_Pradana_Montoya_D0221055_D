public class day26 {
    public static void main(String[] args) {
        double[][] angka = {{1.2 , 2.2 ,2.3},
                           {3.4 , 4.3 ,5.4}};
        for (int i = 0; i < angka.length; i++) {
            for (int j = 0; j < angka.length; j++) {
                System.out.println(angka [i][j]+" ");
                
            }
            
        }
    }
    
}
